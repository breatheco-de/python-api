{
    "name": "flask-microservices-framework",
    "version": 2,
    "builds": [
        {
            "src": "api/*.py",
            "use": "@now/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "methods": ["GET"],
            "dest": "api/index.py"
        }
    ]
}