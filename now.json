{
    "name": "zeit-chat",
    "version": 1.0,
    "builds": [
        {
            "src": "*.py",
            "use": "@now/python"
        }
    ],
    "routes": [
        {
            "src": "/services/_init_.py",
            "methods": ["GET"],
            "dest": "/"
        },
        {
            "src": "/services/user.py",
            "methods": ["POST", "GET"],
            "dest": "/user/"
        }
    ]
}