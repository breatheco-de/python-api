{
    "name": "flask-microservices-framework",
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.py",
            "use": "@liudonghua123/now-flask"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "methods": ["POST", "GET"],
            "dest": "api/*/$1.py"
        }
    ]
}