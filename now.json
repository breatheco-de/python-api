{
    "name": "zeit-chat",
    "version": 2,
    "builds": [
        {
            "src": "/services/user/*.py",
            "use": "@now/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "methods": ["GET"],
            "dest": "/services/user/user.py"
        },
        {
            "src": "/user",
            "methods": ["POST", "GET"],
            "dest": "/services/user/user.py"
        }
    ]
}