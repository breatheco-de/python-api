{
    "name": "zeit-chat",
    "version": 2,
    "builds": [
        {
            "src": "/services/*.py",
            "use": "@now/python"
        },
        {
            "src": "/services/user/*.py",
            "use": "@now/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "methods": ["POST", "GET"],
            "dest": "/services/*/$1.py"
        }
    ]
}